{% layout "server/layouts/base.liquid" %}

{% block content %}
<div class="eventlist">
    <div>
        <ul>
            {% for item in years %}
            {% render "server/components/item-list/item-list.liquid",
            uuid: item.year,
            title: item.year,
            base: "year",
            type: "Year" %}
            {% endfor %}
        </ul>
    </div>
</div>
{% endblock %}